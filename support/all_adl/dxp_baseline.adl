"cartesian plot" {
	object {
		x=0
		y=0
		width=650
		height=350
	}
	plotcom {
		title="$(P)$(R) Baseline Histogram"
		xlabel="Energy (keV)"
		ylabel="Counts"
		clr=14
		bclr=4
	}
	style="line"
	trace[0] {
		xdata="$(P)$(R).BASE_X"
		ydata="$(P)$(R).BASE"
		data_clr=54
	}
	trace[1] {
		xdata="$(P)$(R).BASE_X"
		ydata="$(P)$(R).BASE_CUT"
		data_clr=20
	}
	x_axis {
		rangeStyle="auto-scale"
	}
	y1_axis {
		axisStyle="log10"
		rangeStyle="auto-scale"
	}
	y2_axis {
		rangeStyle="auto-scale"
	}
}
