grecord(ai,"$(P)HeidND261_read")
{
	field(DESC,"PSL theta encoder (raw)")
	field(DTYP,"asyn ai HeidND261")
	field(FLNK,"$(P)HeidND261.PROC PP MS")
	field(INP,"@asyn($(PORT))")
	field(PREC,"6")
	field(EGU,"degrees")
}
grecord(calc,"$(P)HeidND261")
{
	field(DESC,"PSL theta encoder")
	field(FLNK,"$(P)HeidND261_ave.PROC PP MS")
	field(CALC,"k+b*a+c*a^2+d*a^3+e*a^4+f*a^5+g*a^6")
	field(INPA,"$(P)HeidND261_read.VAL NPP NMS")
	field(INPB,"1.0246271")
	field(INPC,"-6.7039952e-05")
	field(INPD,"5.695801e-05")
	field(INPE,"-2.9108104e-07")
	field(INPF,"8.1328257e-09")
	field(INPG,"-2.2046411e-10")
	field(INPK,"18.231947")
	field(EGU,"degrees")
	field(PREC,"5")
}
