	field(INPG,"$(P)HeidND261_ave.LF NPP NMS")
	field(INPH,"$(P)HeidND261_ave.LG NPP NMS")
	field(INPI,"$(P)HeidND261_ave.LH NPP NMS")
	field(INPJ,"$(P)HeidND261_ave.LI NPP NMS")
	field(EGU,"degrees")
	field(PREC,"5")
}
