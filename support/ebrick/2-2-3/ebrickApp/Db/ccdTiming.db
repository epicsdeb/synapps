record(fanout,"$(P)$(E)resetall")
{
    field(DESC, "Reset everything")
    field(SCAN, "Passive")
    field(LNK1, "$(P)$(E)sseq1")
}
record(sseq,"$(P)$(E)sseq1")
{
    field(SCAN, "Passive")
    field(DOL1, "1")
    field(LNK1, "$(P)$(E)reset.PROC PP NMS")
    field(DOL2, "0")
    field(LNK2, "$(P)$(E)mode PP NMS")
    field(DOL3, "0")
    field(LNK3, "$(P)$(E)func PP NMS")
    field(DOL4, "1")
    field(LNK4, "$(P)$(E)dfan1.PROC PP NMS")
    field(DOL5, "1")
    field(LNK5, "$(P)$(E)dfan2.PROC PP NMS")
}
record(longout,"$(P)$(E)reset")
{
    field(DESC, "Reset device")
    field(SCAN, "Passive")
    field(DTYP, "Soft Channel")
    field(VAL,  "1")
    field(OUT,  "$(P)$(R)cntl PP NMS")
}
record(mbbo,"$(P)$(E)mode")
{
    field(DESC, "Mode control")
    field(SCAN, "Passive")
    field(ZRST, "CFG")
    field(ONST, "RUN")
    field(ZRVL, "3")
    field(ONVL, "2")
    field(DTYP, "Raw Soft Channel")
    field(OUT,  "$(P)$(R)cntl PP NMS")
}
record(mbbo,"$(P)$(E)func")
{
    field(DESC, "Function")
    field(SCAN, "Passive")
    field(ZRST, "Disable")
    field(ONST, "CCD-ON")
    field(TWST, "EXT-SYNC")
    field(THST, "CCD-OFF")
    field(ZRVL, "13")
    field(ONVL, "4")
    field(TWVL, "5")
    field(THVL, "6")
    field(DTYP, "Raw Soft Channel")
    field(OUT,  "$(P)$(R)cntl PP NMS")
}
