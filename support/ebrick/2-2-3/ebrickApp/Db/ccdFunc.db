record(stringout,"$(P)$(E)$(F)$(T):name")
{
    field(DESC, "$(F) name")
    field(SCAN, "Passive")
    field(VAL,  "<<<PUT NAME HERE>>>")
}
record(longout,"$(P)$(E)$(F)$(T)")
{
    field(DESC, "$(F) Tx Value")
    field(SCAN, "Passive")
    field(DTYP, "Soft Channel")
    field(OUT,  "$(P)$(E)$(T) PP NMS")
}
record(mbbo,"$(P)$(E)$(F)$(T):units")
{
    field(DESC, "$(F) Tx Units")
    field(SCAN, "Passive")
    field(ZRST, "None")
    field(ONST, "uS")
    field(TWST, "mS")
    field(THST, "S")
    field(ZRVL, "0")
    field(ONVL, "50")
    field(TWVL, "50000")
    field(THVL, "50000000")
    field(DTYP, "Raw Soft Channel")
    field(OUT,  "$(P)$(E)$(T):units PP NMS")
}
