record(ai, "$(P)LAST_ERR")
{
	field(DTYP, "asynInt32")
	field(INP, "@asyn($(PORT),0,0)PI_SUP_LAST_ERR")
	field(SCAN, "I/O Intr")
}

record(bo, "$(P)DEFER") {
  field(DTYP, "asynInt32")
  field(OUT, "@asynMask($(PORT),0,0)MOTOR_DEFER_MOVES")
  field(SCAN, "Passive")
  field(ZNAM, "OFF")
  field(ONAM, "ON")
  field(VAL, "0")
}

