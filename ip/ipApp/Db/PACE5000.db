    field(PINI, "YES")
    field(DTYP, "stream")
    field(OUT,  "@PACE5000.proto setSlewMode $(PORT)")
    field(ZRVL, "0")
    field(ZRST, "MAX")
    field(ONVL, "1")
    field(ONST, "LIN")
}

record(ao,"$(P)$(R)Slew") {
    field(PINI, "YES")
    field(DTYP, "stream")
    field(OUT,  "@PACE5000.proto setSlew $(PORT)")
    field(PREC, "3")
}

