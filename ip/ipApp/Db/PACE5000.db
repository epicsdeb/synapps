    field(ZRVL, "0")
    field(ZRST, "BAR")
    field(ONVL, "1")
    field(ONST, "MPA")
    field(TWVL, "2")
    field(TWST, "KG/CM2")
    field(THVL, "3")
    field(THST, "PSI")
}


# Read settings
record(bo,"$(P)$(R)ReadSettingsScan") {
    field(SCAN, "1 second")
    field(FLNK, "$(P)$(R)ReadSettings.PROC")
}

record(bo,"$(P)$(R)ReadSettings") {
    field(PINI, "YES")
    field(FLNK, "$(P)$(R)Pressure_RBV.PROC")
}

